\@writefile{toc}{\contentsline {subsection}{\numberline {4.1}Show that if \(h \in G\) satisfies \(h \cdot g = g\) for some \(g \in G\), then \(h\) is the identity.}{2}\protected@file@percent }
\@writefile{toc}{\contentsline {subsection}{\numberline {4.2}Fix \(k \in G\). Show that if \(h \cdot k = e\), then \(h = k^{-1}\).}{2}\protected@file@percent }
